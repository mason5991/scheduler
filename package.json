{
  "name": "scheduler",
  "version": "1.0.0",
  "description": "Scheduler service to run jobs",
  "main": "src/index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=local NODE_DEBUG=bull QUEUE_LOG=debug nodemon --watch '*.ts' --exec ts-node src/index.ts",
    "dev": "cross-env NODE_ENV=dev ts-node src/index.ts",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint . --fix",
    "test": "jest --forceExit --detectOpenHandles --coverage --verbose --passWithNoTests",
    "build": "tsc --build tsconfig.build.json",
    "build-test": "cross-env NODE_ENV=production node build/src/index.js",
    "prod": "cross-env NODE_ENV=production PORT=3000 node src/index.js"
  },
  "author": "mason5991",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bull": "^3.22.5",
    "bull-arena": "^3.20.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "md5": "^2.3.0",
    "mongoose": "^5.12.12",
    "multer": "^1.4.2",
    "nanoid": "^3.1.23"
  },
  "devDependencies": {
    "@types/bull": "^3.15.1",
    "@types/bull-arena": "^3.0.3",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/multer": "^1.4.5",
    "@types/newman": "^5.1.3",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-html-reporters": "^2.1.2",
    "jest-sonar-reporter": "^2.0.0",
    "msw": "^0.28.2",
    "newman": "^5.2.3",
    "newman-reporter-htmlextra": "^1.20.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "jestSonar": {
    "reportPath": "reports/jest",
    "reportFile": "jest-report.xml",
    "indent": 2
  }
}
